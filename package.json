{
  "name": "universe-bg",
  "scripts": {
    "start": "./node_modules/.bin/npm-run-all -p dev:server dev:client ",
    "build": "rm dist/*.js && NODE_ENV=production ./node_modules/.bin/webpack -p --progress",
    "dev:server": "node build/dev-server",
    "dev:server-debug": "node --debug build/dev-server",
    "dev:client": "./node_modules/.bin/webpack-dev-server --progress --colors --inline --content-base dist/"
  },
  "description": "this plugin will draw simple universe in canvas with three.js",
  "version": "0.3.0",
  "keywords": [
    "canvas",
    "three.js",
    "universe"
  ],
